.statusbar
    color #000000
    min-height: 21px
    padding-top: 5px
    padding-bottom: 5px
    display: block
    box-shadow: inset 0 1px #f1f1f1;
    background: linear-gradient(#e4e4e4,#b8b7b8);
    border-radius: 4px 4px 0 0;
    height: 0.8em

.container
    padding-right: 15px
    padding-left: 15px
    margin-right: auto
    margin-left: auto
    @media (min-width: 576px)
        width: 540px
        max-width: 540px
    @media (min-width: 770px)
        width: 750px
        max-width: 720px
    @media (min-width: 992px)
        width: 970px
        max-width: 960px
    @media (min-width: 1200px)
        width: 1170px
        max-width: 1140px

.content
    margin: 0 auto
    overflow hidden
    background: hsla(0,0%,5%,.75);
    box-shadow: 0 3px 6px rgba(0,0,0,.16), 0 3px 6px rgba(0,0,0,.23);
    border-radius: 0 0 5px 5px

.credits-content
    padding: 15px 0px 30px 0px
    font-size: 16px
    line-height: 1.75
    color: #f1f1f1

.row
    display: flex

.col-md-1
    max-width: 33.33333%
    width: 33.33333%
    margin-right: 0.5em
    

.col-element
    margin: 0 0 5px
    font-size: 12px
    // font-weight: 500;

span
    margin-left: 5px
